# focus windows
alt - left : chunkc tiling::window --focus west
alt - down : chunkc tiling::window --focus south
alt - up : chunkc tiling::window --focus north
alt - right : chunkc tiling::window --focus east

# move window
alt + lshift - left : chunkc tiling::window --warp west
alt + lshift - down : chunkc tiling::window --warp south
alt + lshift - up : chunkc tiling::window --warp north
alt + lshift - right : chunkc tiling::window --warp east

# toggle desktop offset
alt - a : chunkc tiling::desktop --toggle offset

# toggle window native fullscreen
alt - f : chunkc tiling::window --toggle native-fullscreen
#alt - f : chunkc tiling::window --toggle fullscreen

# float / unfloat window
alt + shift - f : chunkc tiling::window --toggle float

# toggle window parent zoom
#alt - d : chunkc tiling::window --toggle parent

# toggle window split type
alt - e : chunkc tiling::window --toggle split

